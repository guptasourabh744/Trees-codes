int findCeil(Node* root, int input) {
    if (root == NULL) return -1;

   int ceil = -1;
   
   while(root != NULL) {
       if( input == root->data) {
           ceil = root->data;
           return ceil;
       }
       
       else if(input > root->data) {
           root = root->right;
       }
       else{
           ceil = root->data;
           root = root->left;
       }
   }
   return ceil;
}
